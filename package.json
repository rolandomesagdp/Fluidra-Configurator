{
  "name": "fluidra-configurator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:libs && npm run start:uiPro",
    "dev": "concurrently \"npm run watch:libs\" \"npm run start:uiPro\"",
    "start:uiPro": "ng serve ui-pro --configuration development",
    "start:uiElement": "ng serve ui-element --configuration development",
    "build:uiPro": "npm run build:libs && ng build ui-pro --configuration production",
    "build:uiElement": "npm run build:libs && ng build ui-element --configuration production",
    "build:libs": "npm run build:core && npm run build:analysisForms",
    "build:core": "ng build core --configuration production",
    "build:analysisForms": "ng build analysis-forms --configuration production",
    "test": "npm run test:core && npm run test:analysisForms && npm run test:uiPro && npm run test:uiElement",
    "test:core": "ng test core --no-watch --no-progress --browsers ChromeHeadless --source-map=false",
    "test:analysisForms": "COVERAGE_DIR=analysis-forms ng test analysis-forms --no-watch --no-progress --browsers ChromeHeadlessCI --source-map=false --code-coverage",
    "test:uiPro": "COVERAGE_DIR=ui-pro ng test ui-pro --no-watch --no-progress --browsers ChromeHeadlessCI --source-map=false --code-coverage",
    "test:uiElement": "ng test ui-element --no-watch --no-progress --browsers ChromeHeadless --source-map=false",
    "dev:test": "concurrently \"npm run dev:test:core\" \"npm run dev:test:analysisForms\" \"npm run dev:test:uiPro\" \"npm run dev:test:uiElement\"",
    "dev:test:core": "set COVERAGE_DIR=core&&  ng test core --watch --browsers Chrome  --code-coverage",
    "dev:test:analysisForms": "set COVERAGE_DIR=analysis-forms&&  ng test analysis-forms --watch --browsers Chrome  --code-coverage",
    "dev:test:uiPro": "set COVERAGE_DIR=ui-pro&&  ng test ui-pro --watch --browsers Chrome  --code-coverage",
    "dev:test:uiElement": "set COVERAGE_DIR=ui-element&&  ng test ui-element --watch --browsers Chrome  --code-coverage",
    "watch:libs": "concurrently \"npm run watch:core\" \"npm run watch:analysisForms\"",
    "watch:core": "ng build core --watch --configuration development",
    "watch:analysisForms": "ng build analysis-forms --watch --configuration development",
    "lint": "eslint . --ext .ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.3",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.0.3",
    "@angular/compiler": "^17.0.3",
    "@angular/core": "^17.0.3",
    "@angular/elements": "^17.0.3",
    "@angular/forms": "^17.0.3",
    "@angular/material": "^17.0.1",
    "@angular/material-moment-adapter": "^17.0.1",
    "@angular/platform-browser": "^17.0.3",
    "@angular/platform-browser-dynamic": "^17.0.3",
    "@angular/router": "^17.0.3",
    "@auth0/auth0-angular": "^2.2.1",
    "@fontsource/work-sans": "^5.0.12",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.1",
    "@angular/cli": "~17.0.1",
    "@angular/compiler-cli": "^17.0.3",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "concurrently": "^8.2.1",
    "eslint": "^8.52.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.0.1",
    "typescript": "~5.2.2"
  }
}
