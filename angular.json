          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/configurator-ui-pro/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/configurator-ui-pro/src/favicon.ico",
              "projects/configurator-ui-pro/src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/pink-bluegrey.css",
              "projects/configurator-ui-pro/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "configurator-components": {
      "projectType": "library",
      "root": "projects/configurator-components",
      "sourceRoot": "projects/configurator-components/src",
      "prefix": "fcc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/configurator-components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/configurator-components/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/configurator-components/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/configurator-components/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "configurator-core": {
      "projectType": "library",
      "root": "projects/configurator-core",
      "sourceRoot": "projects/configurator-core/src",
      "prefix": "ffc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/configurator-core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/configurator-core/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/configurator-core/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/configurator-core/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
